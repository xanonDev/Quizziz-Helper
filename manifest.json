{
	"manifest_version": 3,
	"name": "Quizizz Helper",
	"version": "1.7",
	"description": "Extracts answers for Quizizz quizzes, author x__anon",
	"content_scripts": [
		{
			"matches": ["https://*.quizizz.com/*"],
			"js": ["script.js"],
			"run_at": "document_end"
		}
	],
	"permissions": ["activeTab", "https://api.quizit.online/*"],
	"action": {
		"default_icon": "icon.png"
	},
	"icons": {
		"128": "icon.png",
		"16": "icon.png",
		"48": "icon.png"
	}
}
